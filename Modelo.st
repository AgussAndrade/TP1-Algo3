Object subclass: #AlgoFix	instanceVariableNames: 'carpeta'	classVariableNames: ''	package: 'TP1-Modelo'!!AlgoFix methodsFor: 'initialization' stamp: 'Andra 5/26/2020 16:33'!initialize	carpeta := CarpetaVacia new.! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'Andra 5/18/2020 16:22'!crearPintura: marca conPrecioPorLitro: precio manosPincel: pinceles manosRodillo: rodillos ^Pintura crear: marca conPrecioPorLitro: precio manosPincel: pinceles manosRodillo: rodillos.! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'Andra 6/18/2020 21:21'!registrarPintorDeRodillo: unNombre conValorHora: unSalario	carpeta := carpeta registrar:(Pintor conNombre: unNombre cobra: unSalario  yTecnica: (RubroRodillo new))! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'Andra 5/26/2020 16:33'!manoDeObra: valorManodeObra masMateriales: valorMateriales	^valorManodeObra +valorMateriales.! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'Andra 6/18/2020 21:20'!registrarPintorDePincel: nombre conValorHora: valor	carpeta := carpeta registrar:(Pintor conNombre: nombre cobra: valor yTecnica: (RubroPincel new)).! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'Andra 5/19/2020 00:48'!presupuestoMasBaratoParaPintarMetrosCuadrados: metros conPintura: pintura^ carpeta presupuestoMasBaratoParaPintarMetrosCuadrados: metros conPintura: pintura! !Object subclass: #CarpetaConPintores	instanceVariableNames: 'registro'	classVariableNames: ''	package: 'TP1-Modelo'!!CarpetaConPintores methodsFor: 'setter' stamp: 'Andra 6/17/2020 17:45'!registro: aRegistrar	registro := OrderedCollection new.	registro add: aRegistrar! !!CarpetaConPintores methodsFor: 'as yet unclassified' stamp: 'Andra 6/18/2020 21:19'!registrar: pintor	registro add: pintor.	^self! !!CarpetaConPintores methodsFor: 'as yet unclassified' stamp: 'Andra 6/18/2020 21:22'!presupuestoMasBaratoParaPintarMetrosCuadrados: metros conPintura: pintura	|pintorMasBarato |	pintorMasBarato := registro detectMin: [ :unPintor | 		unPintor calcularPresupuestoDe: metros Con: pintura].	^ Presupuesto crearConNombre: (pintorMasBarato responsable) conValor: (pintorMasBarato calcularPresupuestoDe: metros Con: pintura).! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!CarpetaConPintores class	instanceVariableNames: ''!!CarpetaConPintores class methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 17:42'!crearCon: pintor	|carpeta|	carpeta := self new.	carpeta := carpeta registro: pintor. 	^carpeta! !Object subclass: #CarpetaVacia	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Modelo'!!CarpetaVacia methodsFor: 'as yet unclassified' stamp: 'Andra 6/18/2020 21:21'!registrar: pintor	^ CarpetaConPintores crearCon: pintor! !!CarpetaVacia methodsFor: 'as yet unclassified' stamp: 'Andra 5/26/2020 16:34'!presupuestoMasBaratoParaPintarMetrosCuadrados: metros conPintura: pintura	PintorNoRegistradoError signal! !Object subclass: #Descuento	instanceVariableNames: 'descuento'	classVariableNames: ''	package: 'TP1-Modelo'!!Descuento methodsFor: 'as yet unclassified' stamp: 'Andra 6/19/2020 16:42'!aplicarPara: metros con: ultimoPresupuesto (metros > 40) ifTrue: [^ ultimoPresupuesto * descuento ].^ultimoPresupuesto ! !!Descuento methodsFor: 'initialization' stamp: 'Andra 6/19/2020 16:43'!initialize descuento := 0.5! !Object subclass: #Pintor	instanceVariableNames: 'id cobra habilidad'	classVariableNames: ''	package: 'TP1-Modelo'!!Pintor methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 19:32'!nombrar: nombre ponerSalario: valor yAgregarTecnica: tecnica	habilidad := tecnica.	cobra:=valor.	id:=nombre.! !!Pintor methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 18:21'!calcularPresupuestoDe: metros Con:pintura	^ habilidad calcularPresupuestoDe: metros Con: pintura para: cobra! !!Pintor methodsFor: 'as yet unclassified' stamp: 'Andra 5/26/2020 16:35'!responsable	^id! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintor class	instanceVariableNames: ''!!Pintor class methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 17:48'!conNombre: nombre yCobra: valor	|pintor|	pintor:= self new.	pintor nombrar: nombre yponerSalario: valor.	^pintor! !!Pintor class methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 19:32'!conNombre: nombre cobra: valor yTecnica: tecnica	|pintor|	pintor:= self new.	pintor nombrar: nombre ponerSalario: valor yAgregarTecnica: tecnica.	^pintor! !Error subclass: #PintorNoRegistradoError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Modelo'!Object subclass: #Pintura	instanceVariableNames: 'valor manosConPincel manosConRodillo'	classVariableNames: ''	package: 'TP1-Modelo'!!Pintura methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 17:52'!agregarManosPincel: pinceladas conManosRodillo: rodilladas yPrecioPorLitro: precio	valor := precio.	manosConRodillo := rodilladas.	manosConPincel := pinceladas.! !!Pintura methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 18:17'!precio^ valor! !!Pintura methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 19:16'!manosPincel^ manosConPincel! !!Pintura methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 19:16'!manosRodillo^ manosConRodillo! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintura class	instanceVariableNames: ''!!Pintura class methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 18:46'!crear: nombre conPrecioPorLitro: precio manosPincel: pinceladas manosRodillo: rodilladas 	|pintura|	pintura := self new.	pintura agregarManosPincel: pinceladas conManosRodillo: rodilladas yPrecioPorLitro: precio.	^pintura! !Object subclass: #Presupuesto	instanceVariableNames: 'valor responsable'	classVariableNames: ''	package: 'TP1-Modelo'!!Presupuesto methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 19:49'!ponerNombre: nombre conValor: salario	valor := salario.	responsable := nombre! !!Presupuesto methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 19:48'!responsable^responsable! !!Presupuesto methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 19:48'!valor^valor! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Presupuesto class	instanceVariableNames: ''!!Presupuesto class methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 19:48'!crearConNombre: nombre conValor: salario	|presupuesto|	presupuesto := Presupuesto new.	presupuesto ponerNombre: nombre conValor: salario.	^presupuesto! !Object subclass: #RubroPincel	instanceVariableNames: 'descuento'	classVariableNames: ''	package: 'TP1-Modelo'!!RubroPincel methodsFor: 'initialization' stamp: 'Andra 6/18/2020 19:56'!initialize descuento := Descuento new. ! !!RubroPincel methodsFor: 'as yet unclassified' stamp: 'Andra 6/18/2020 19:59'!calcularPresupuestoDe: metros Con:pintura para:salario	|valorPintura manos ultimoPresupuesto|	manos := pintura manosPincel.	valorPintura := pintura precio.	ultimoPresupuesto := (manos * 2 * salario * metros). 	ultimoPresupuesto := (descuento aplicarPara: metros con: ultimoPresupuesto) + (metros * 4 * valorPintura  * manos).	^ultimoPresupuesto ! !Object subclass: #RubroRodillo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Modelo'!!RubroRodillo methodsFor: 'as yet unclassified' stamp: 'Andra 6/17/2020 19:26'!calcularPresupuestoDe: metros Con:pintura para:salario	|valorPintura manos|	manos  := pintura manosRodillo.	valorPintura := pintura precio.	^(metros * 5 * valorPintura  * manos) + (manos * 1 * salario * metros).! !